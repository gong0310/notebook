,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;cACrC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;cACjB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;YACnB,CAAC,CAAC,CAAC,CAAC;UACN,CAAC,CAAC,CAAC,CAAC,CAAC;QACP,CAAC,CAAC,CAAC,CAAC;MACN,CAAC,CAAC,CAAC,CAAC;IACN,CAAC,CAAC,CAAC,CAAC,CAAC;EACP,CAAC,CAAC,CAAC,CAAC,CAAC","file":"C:/Users/26917/Desktop/mydemo/notebook/view/src/views/Home.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"home\">\n    <div class=\"header\">\n      <div class=\"title\">\n        便签\n        <span @click=\"add\">\n          <i class=\"fa fa-plus\" aria-hidden=\"true\"></i>\n        </span>\n      </div>\n      <p>便签数</p>\n      <div class=\"count\">\n        <span>{{list.length}}</span>个\n      </div>\n      <div class=\"tis\">公开{{list.length}}个</div>\n    </div>\n    <div class=\"content\">\n      <ul>\n        <li\n          v-for=\"i in list\"\n          :key=\"i._id\"\n          @tap=\"edit(i._id,$event)\"\n        >\n          <div class=\"l\">\n            <div class=\"title\">{{i.title}}</div>\n            <div class=\"date\">{{i.dates}}</div>\n          </div>\n          <div class=\"r\"\n          \n          @touchend=\"end\"\n          @touchstart=\"start\"\n          @touchmove=\"move\">\n            <i\n              @click=\"change(i._id,$event)\"\n              :style=\"i.isstar?'color: #fdb238;':''\"\n              class=\"fa fa-star\"\n              aria-hidden=\"true\"\n            ></i>\n          </div>\n        </li>\n      </ul>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { getList } from \"../api\";\nimport { onMounted, reactive, toRefs } from \"vue\";\nimport { useRouter } from \"vue-router\";\nexport default {\n  name: \"Home\",\n  setup() {\n    let router = useRouter();\n    const state = reactive({\n      list: [],\n      startX: null,\n      startY: null,\n    });\n    const move = (e) => {\n      // e.preventDefault(e);\n      let moveEndX, moveEndY, X, Y;\n      moveEndX = e.changedTouches[0].pageX;\n      moveEndY = e.changedTouches[0].pageY;\n      X = moveEndX - state.startX;\n      Y = moveEndY - state.startY;\n\n      if (Math.abs(X) > Math.abs(Y) && X > 50) {\n        // alert(\"左往右滑\");\n      } else if (Math.abs(X) > Math.abs(Y) && X < -50) {\n        console.log(\"右往左滑\");\n      } else if (Math.abs(Y) > Math.abs(X) && Y > 0) {\n        // console.log(\"上往下滑\");\n      } else if (Math.abs(Y) > Math.abs(X) && Y < 0) {\n        // console.log(\"下往上滑\");\n      } else {\n        // console.log(\"滑了个寂寞\");\n      }\n    };\n    const start = (e) => {\n      state.pressTime = 0;\n      state.pressTimer = setInterval(() => {\n        state.pressTime = state.pressTime + 1;\n      }, 100);\n      document.addEventListener(\"touchmove\",()=>{}, false);\n      e.preventDefault();\n      (state.startX = e.changedTouches[0].pageX),\n        (state.startY = e.changedTouches[0].pageY);\n    };\n    const end = () => {\n      clearInterval(state.pressTimer);\n      if (state.pressTime < 1) {\n        // 长按时间不足500ms时，触发点击事件\n        console.log(\"取消\");\n      }\n    };\n    const add = () => {\n      router.push(\"/add\");\n    };\n    const change = (id, e) => {\n      state.list.map((i) => {\n        if (i._id == id) {\n          i.isstar = !i.isstar;\n        }\n        return i;\n      });\n      e.stopPropagation();\n      state.isstar = !state.isstar;\n    };\n    const edit = (id, e) => {\n      e.stopPropagation();\n      router.push({\n        path: \"/add\",\n        query: {\n          _id: id,\n        },\n      });\n    };\n\n    const getdata = () => {\n      getList().then((res) => {\n        state.list = res.data.map((i) => {\n          i.isstar = false;\n          let year = new Date(i.date).getFullYear();\n          let month = new Date(i.date).getMonth() + 1;\n          let date = new Date(i.date).getDate();\n          let hour = new Date(i.date).getHours();\n          let min = new Date(i.date).getMinutes();\n          i.dates =\n            year + \"-\" + month + \"-\" + date + \" \" + hour + \":\" + min + \"分\";\n          return i;\n        });\n      });\n    };\n    onMounted(() => {\n      getdata();\n    });\n    return {\n      ...toRefs(state),\n      add,\n      edit,\n      change,\n      start,end,\n      move,\n    };\n  },\n};\n</script>\n<style lang=\"less\" scoped>\n.home {\n  width: 100%;\n  min-height: 100vh;\n  background-color: #f9f9f9;\n\n  .header {\n    width: 100%;\n    height: 30%;\n    background-image: url(\"../assets/img/bc.jpg\");\n    background-size: cover;\n    background-repeat: no-repeat;\n    background-position-x: 50%;\n    color: #fff;\n    .title {\n      padding: 0.533333rem 0;\n      box-sizing: border-box;\n      font-size: 0.96rem;\n      // font-weight: 600;\n      position: relative;\n      span {\n        position: absolute;\n        box-sizing: border-box;\n        right: 0.533333rem;\n        color: #fff;\n        cursor: pointer;\n      }\n    }\n    p {\n      text-align: left;\n      padding-left: 0.8rem;\n      box-sizing: border-box;\n    }\n    .count {\n      height: 3.2rem;\n      text-align: left;\n      padding: 0.533333rem 0;\n      padding-left: 0.8rem;\n      box-sizing: border-box;\n      span {\n        font-size: 2.613333rem;\n      }\n    }\n    .tis {\n      text-align: left;\n      padding: 1.066667rem 0.8rem;\n      box-sizing: border-box;\n    }\n  }\n  .content {\n    margin: 0.533333rem;\n    box-sizing: border-box;\n    ul {\n      margin: 0;\n      li {\n        width: 100%;\n        margin: 0.533333rem 0;\n        height: 3.2rem;\n        background-color: #fff;\n        border-radius: 0.266667rem;\n        box-shadow: 0 3px 3px rgba(204, 204, 204, 0.14),\n          0 0 5px rgba(204, 204, 204, 0.12);\n        position: relative;\n        overflow: hidden;\n        &::after {\n          content: \"\";\n          display: inline-block;\n          width: 0.266667rem;\n          height: 100%;\n          background-color: #b0d6e1;\n          position: absolute;\n          left: 0;\n        }\n        .l {\n          float: left;\n          font-size: 0.853333rem;\n          text-align: left;\n          padding: 0.266667rem;\n          padding-left: 0.8rem;\n          box-sizing: border-box;\n          height: 100%;\n          div {\n            height: 1.4rem;\n            line-height: 1.4rem;\n          }\n          div:nth-child(2) {\n            color: #ccc;\n            font-size: 0.746667rem;\n          }\n        }\n        .r {\n          float: right;\n          padding-right: 0.8rem;\n          height: 100%;\n          line-height: 3rem;\n          font-size: 0.8rem;\n        }\n      }\n    }\n  }\n}\n</style>\n"]}]}